  	<Button android:text="@string/opr_sqr" android:id="@+id/key_sqr" android:width="0dip" android:layout_height="@dimen/operatorkeypad_rowheight" android:textSize="@dimen/operatorkeypad_textsize"/>
  	<Button android:text="@string/opr_log10" android:id="@+id/key_log10" android:width="0dip" android:layout_height="@dimen/operatorkeypad_rowheight" android:textSize="@dimen/operatorkeypad_textsize"/>
  </TableRow>
  <TableRow>
  	<Button android:text="@string/opr_sin" android:id="@+id/key_sin" android:width="0dip" android:layout_height="@dimen/operatorkeypad_rowheight" android:textSize="@dimen/operatorkeypad_textsize"/>
  	<Button android:text="@string/opr_cos" android:id="@+id/key_cos" android:width="0dip" android:layout_height="@dimen/operatorkeypad_rowheight" android:textSize="@dimen/operatorkeypad_textsize"/>
  	<Button android:text="@string/opr_tan" android:id="@+id/key_tan" android:width="0dip" android:layout_height="@dimen/operatorkeypad_rowheight" android:textSize="@dimen/operatorkeypad_textsize"/>
