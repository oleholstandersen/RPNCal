  </TableRow>
  <TableRow>
  	<Button android:text="@string/opr_asin" android:id="@+id/key_asin" android:width="0dip" android:layout_height="@dimen/operatorkeypad_rowheight" android:textSize="@dimen/operatorkeypad_textsize"/>
