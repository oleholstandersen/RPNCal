<?xml version="1.0" encoding="utf-8"?>
<TableLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="@dimen/operatorkeypad_width"
  android:layout_height="@dimen/operatorkeypad_height"
  android:stretchColumns="*" android:id="@+id/operators_view"
  android:textSize="@dimen/operatorkeypad_textsize">
  <TableRow>
  	<Button android:text="@string/opr_inv" android:id="@+id/key_inv" android:width="0dip" android:layout_height="@dimen/operatorkeypad_rowheight" android:textSize="@dimen/operatorkeypad_textsize"/>
  	<Button android:text="@string/opr_pow" android:id="@+id/key_pow" android:width="0dip" android:layout_height="@dimen/operatorkeypad_rowheight" android:textSize="@dimen/operatorkeypad_textsize"/>
  	<Button android:text="@string/opr_sqrt" android:id="@+id/key_sqrt" android:width="0dip" android:layout_height="@dimen/operatorkeypad_rowheight" android:textSize="@dimen/operatorkeypad_textsize"/>
  	<Button android:text="@string/opr_exp10" android:id="@+id/key_exp10" android:width="0dip" android:layout_height="@dimen/operatorkeypad_rowheight" android:textSize="@dimen/operatorkeypad_textsize"/>
  </TableRow>
  <TableRow>
  	<Button android:text="@string/opr_pi" android:id="@+id/key_pi" android:width="0dip" android:layout_height="@dimen/operatorkeypad_rowheight" android:textSize="@dimen/operatorkeypad_textsize"/>
