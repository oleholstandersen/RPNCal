  	<Button android:text="@string/opr_atan" android:id="@+id/key_atan" android:width="0dip" android:layout_height="@dimen/operatorkeypad_rowheight" android:textSize="@dimen/operatorkeypad_textsize"/>
  	<Button android:text="@string/opr_log" android:id="@+id/key_log" android:width="0dip" android:layout_height="@dimen/operatorkeypad_rowheight" android:textSize="@dimen/operatorkeypad_textsize"/>
  </TableRow>
</TableLayout>
